<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="it.geosolutions.ie.model.Computation" table="IE_COMPUTATION"
        discriminator-value="0">
  <id name="id" column="ID">
   <generator class="native" />
  </id>
  <property name="started" column="STARTED_COL" />
  <property name="finished" column="FINISHED_COL" />
  <property name="status" column="STATUS_COL" />
 </class>
</hibernate-mapping>

